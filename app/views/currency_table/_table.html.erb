<table class="table table-bordered table-hover">
  <thead>
    <tr class="table-dark">
      <% @table_data[:header].each do |header| %>
        <th scope="col" class="px-3"><%= header %></th>
      <% end %>
    </tr>
  </thead>
  <tbody>
    <% @table_data[:currencies].each_with_index do |currency, index| %>
      <tr>
        <% currency.each do |cell| %>
          <td<%= ' scope="row"' if index.zero? %> class="px-3"><%= cell %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>
